# Environment Variables Example
# Copy this file to .env and fill in your specific values

# API Configuration
API_URL=http://localhost:8000
API_KEY=your_api_key_here

# Supabase Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_KEY=your_supabase_key

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key

# Microsoft Entra ID Configuration
MS_TENANT_ID=your_tenant_id
MS_CLIENT_ID=your_client_id
MS_CLIENT_SECRET=your_client_secret
MS_REDIRECT_URI=http://localhost:3000/auth/callback

# Halo ITSM Configuration
HALO_API_URL=your_halo_api_url
HALO_API_KEY=your_halo_api_key
HALO_CLIENT_ID=your_halo_client_id

# SharePoint Configuration
SHAREPOINT_SITE_URL=your_sharepoint_site_url

# Power BI Configuration
POWERBI_WORKSPACE_ID=your_powerbi_workspace_id

# Application Configuration
PORT=8000
HOST=0.0.0.0
NODE_ENV=development
